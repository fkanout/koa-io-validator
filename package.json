{
  "name": "koa-validator",
  "version": "1.0.0",
  "description": "Koa input/output validation middleware",
  "main": "lib/",
  "scripts": {
    "dev": "nodemon --exec babel-node src/",
    "build": "babel src --out-dir lib --source-maps",
    "start": "npm run build && node lib/",
    "test": "nyc mocha test/ --timeout 10000"
  },
  "keywords": [
    "koa",
    "validation",
    "input",
    "output",
    "joi"
  ],
  "author": "Faisal KANOUT",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@hapi/joi": "^16.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@koa/router": "^8.0.5",
    "axios": "^0.19.0",
    "got": "^10.1.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
